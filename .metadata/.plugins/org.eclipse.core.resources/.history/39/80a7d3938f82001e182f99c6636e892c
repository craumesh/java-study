class NormalParent2 {
	public void member() {
		System.out.println("디자인을 합니다!");
	}
}

class Worker {
	
	public void member(NormalParent parent) {		
		System.out.println("디자인을 합니다!");
	};
	
	public void method1() {
		member(new NormalParent2()){
			
		};
		System.out.println("테스트를 합니다!");
	}
	
	public void start() {
		method1();
		method2();
	}
}