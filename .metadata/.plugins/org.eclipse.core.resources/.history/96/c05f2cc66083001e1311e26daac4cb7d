package lambda;

import java.util.Arrays;

public class Ex4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * 람다식 응용
		 */
		
		String[] str = {"this", "is", "java", "world"};
		
		System.out.println(Arrays.toString(str));
		
		Arrays.sort(str);
		System.out.println(Arrays.toString(str));
		
		/*
		 * 새로운 정렬기능을 만들려면?
		 * => 내부적으로 Comparator 의 compare() 메서드를 사용
		 */
	}
}
