}

interface Vehicle {
	void run();
}

class AnonymousClass {
	
	void method1() {
		Vehicle vc = new Vehicle() {		
			@Override
			public void run() {
				// TODO Auto-generated method stub
				System.out.println("자전거가 달립니다!");
			}
		};
		vc.run();
	}
	
	
	
}