package lambda;

public class Test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Str str = ("안녕", "하세요") -> x + y ;
		StringLink sl = new StringLink();
		System.out.println(sl.linkString("안녕", "하세요!"));
	}

}

interface Str {
	String link(String x, String y);
}

class StringLink {
	String linkString(String x, String y) {
		return x + "" + y;
	}
}