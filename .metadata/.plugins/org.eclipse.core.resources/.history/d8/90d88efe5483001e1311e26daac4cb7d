package lambda;

public class Test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Str str = (x, y) -> x + y ;
		StringLink sl = (StringLink) new Str();
		System.out.println(str.link("안녕", "하세요!"));
	}

}

interface Str {
	String link(String x, String y);
}

class StringLink {
	String link(String x, String y) {
		return x + "" + y;
	}
}